#include <stdio.h>

int sum_from_to(int x, int y)
{
    return ((y+1)*y/2)-((x-1)*x/2);
}

void test_sum_from_to(void)
{
    int x;
    scanf("%d", &x);
    int y;
    scanf("%d", &y);
    int z = sum_from_to(x,y);
    printf("%d\n", z);
}

int main(void)
{
	//test_sum_naturals();
	test_sum_from_to();
	return 0;
}
